@model DeckBuilder.Models.GameVersion

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>GameVersion</legend>

    <div class="display-label">VersionString</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.VersionString)
    </div>

    <div class="display-label">PythonScript</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.PythonScript)
    </div>

    <div class="display-label">MaxPlayers</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.MaxPlayers)
    </div>

    <div class="display-label">CreationDate</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.CreationDate)
    </div>
