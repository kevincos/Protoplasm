<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/GamePage.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Protoplasm.css")" rel="stylesheet" type="text/css" />
    
    <link href="@Url.Content("~/Content/openid-shadow.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/openid.css")" rel="stylesheet" type="text/css" />

    


    <script src="@Url.Content("https://www.google.com/jsapi")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-1.6.4.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.signalR.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/lobbychat.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/play.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/waitingRoom.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/signalr/hubs")" type="text/javascript"></script></head>
    <script src="@Url.Content("~/Scripts/Math/vector.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Math/hexmath.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Graphics/sprite.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Graphics/spritecontext.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/GameCore/gamecore.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/GameCore/gamecore.hand2.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/GameCore/gamecore.hexboard2.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/GameCore/gamecore.squareboard2.js")" type="text/javascript"></script>   
    <script src="@Url.Content("~/Scripts/GameCore/gamecore.infowindow2.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/GameCore/gamecore.track.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/GameCore/gamecore.main.js")" type="text/javascript"></script>

    <script src="@Url.Content("~/Scripts/openid-jquery.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/openid-en.js")" type="text/javascript"></script>

    <script type="text/javascript">
        $(document).ready(function () {
            openid.init('openid_identifier');
        });
    </script>
</head>
<body>
    <div class = "content">
        <div class="page">
            <header>
                <div id="logindisplay">
                    @Html.Partial("_LogOnPartial")
                </div>
                <div id="navbar">
                    <div class="logo">
                        Protoplasm
                    </div>
                    <ul id="menu">
                        <li>@Html.ActionLink("News", "Index", "Home")</li>
                        <li>@Html.ActionLink("Lobby", "Lobby", "Home")</li>                        
                        <li>@Html.ActionLink("Games", "Index", "Game")</li>                    
                        <li>@Html.ActionLink("Players", "Index", "Player")</li>
                        <li>@Html.ActionLink("Profile", "Profile", "Home")</li>                    
                        <li>@Html.ActionLink("Developer", "DeveloperProfile", "Home")</li>                        
                    </ul>
                </div>
            </header>
            <section id="main">
                @RenderBody()
            </section>

            <footer>
            </footer>
        </div>
    </div>
</body>
</html>
